
public class Greeter {

	//instance variable
	public String greeting;
	
	
	//getter & setter
	public String getGreeting() {
		return greeting;
	}
	
	public void setGreeting	() {
		this.greeting = greeting;
	}
	
	//constructor
	public Greeter(String greeting) {
		this.greeting = greeting;
		
	}

	
	  @Override public String toString() {
		  return greeting; 
		  
	  }
	 
	
	
}
